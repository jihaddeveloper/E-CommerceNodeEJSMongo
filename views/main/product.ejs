<% layout('layoutPage') -%>

<div class="container" style="width:70%; min-height: 700px">
    <div class="row">
        <div class="col-md-4">
            <img src="<%= product.image %>">
        </div>
    </div>
    <div class="row">
        <div class="col-md-4">
            <h3>
                <%= product.name %>
            </h3>
            <h1>$
                <%= product.price %>.00
            </h1>
        </div>
    </div>
</div>